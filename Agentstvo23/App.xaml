<Application x:Class="Agentstvo23.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Agentstvo23"
             xmlns:vm="clr-namespace:Agentstvo23.ViewModels"
             StartupUri="Views/Windows/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Styles/StylesDictionary.xaml"/>
                <ResourceDictionary Source="/Resources/Templates/MainTemplates.xaml"/>
                <ResourceDictionary Source="/Resources/Styles/MainColors.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
                <vm:ViewModelLocator x:Key="Locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
